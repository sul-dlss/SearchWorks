<%= link_to see_all_link, class: "btn btn-outline-secondary text-nowrap" do %>
 	<% case total -%>
	<% when 1 %>
    See <%= number_with_delimiter(total) %> <%= visually_hidden_service(service_name) %> result
	<% when 2, 3 %>
	  See <%= number_with_delimiter(total) %> <%= visually_hidden_service(service_name) %> results
  <% else %>
    See all <%= number_with_delimiter(total) %> <%= visually_hidden_service(service_name) %> results
  <% end %>
<% end %>
