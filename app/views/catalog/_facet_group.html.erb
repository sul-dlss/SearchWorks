<% # overriding Blacklight for two reasons:
   #  1. to pass a different title
   #  2. to pass a list of facets (not facet-names), like Blackight 8, to deconflict the two date facets%>
<%= render Searchworks::Response::FacetGroupComponent.new(
  response: @response,
  id: groupname,
  title: t("blacklight.search.facets.access_point.#{page_location.access_point}.title",
          access_point: page_location.access_point,
          default: 'Limit your search')) do |component| %>
    <% component.with_body do %>
      <%= render Blacklight::FacetComponent.with_collection(
        blacklight_config.facet_fields.values.select { |opts| groupname == opts[:group] },
       response: @response) %>
    <% end %>
<% end %>