<html>
  <head>
    <style>
      .side-nav-minimap { display: none; }
    </style>
  </head>
  <body>
    <% if @message.present? || @email_from.present? %>
      <dl>
        <% if @email_from.present? %>
          <dt>Email from</dt>
          <dd><%= @email_from %></dd>
        <% end %>
        <% if @message.present? %>
          <dt>Message</dt>
          <dd><%= @message %></dd>
        <% end %>
      </dl>
    <% end %>
    <% @documents.each do |document| %>
      <h1><a href="<%= polymorphic_url(document, {:only_path => false}.merge(@url_gen_params)) %>"><%= document[:eds_title] %></a></h1>
      <% if document.access_panels.online? %>
        <h2>Online</h2>
        <% document.eds_links.fulltext.each do |link| %>
          <div><a href="<%= link.href %>"><%= link.text %></a></div>
        <% end %>
      <% end %>
      <hr>
    <% end %>
  </body>
</html>
