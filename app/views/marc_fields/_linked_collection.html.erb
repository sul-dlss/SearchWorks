<% if linked_collection.values.present? %>
  <dt class="col-md-3"><%= linked_collection.label %></dt>
  <% linked_collection.values.each do |value| %>
    <dd class="col-md-9">
      <%= link_to(value, search_catalog_path(search_field: :subject_terms, q: "\"#{value}\"")) %>
    </dd>
  <% end  %>
<% end %>
