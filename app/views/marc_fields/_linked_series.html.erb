<dt class="col-md-4"><%= linked_series.label %></dt>
<% linked_series.values.each do |series| %>
  <dd class="col-md-8">
    <% if series[:link] %>
      <%= link_to(series[:link].strip, search_catalog_path(q: "\"#{series[:link].strip}\"", search_field: 'search_series')) %>
    <% end %>
    <%= series[:extra_text] %>
  </dd>
<% end %>
