<%= render @layout.new(classes: 'panel-appears-in') do |c| %>
  <% c.title { 'Item is included in another record' } %>
  <% c.body do %>
    <% @document.parent_sets.each do |parent_set| %>
      <h4>
        <%= helpers.render_resource_icon parent_set[parent_set.format_key] %>
        <%= helpers.link_to_document(parent_set) %>
      </h4>
    <% end %>
  <% end %>
<% end %>
